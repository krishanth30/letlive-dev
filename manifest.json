{
  "manifest_version": 2,
  "name": "Let Live",
  "description": "This extension escapes server session timeouts",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["utils.js", "jquery.min.js", "jquery-ui.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
	"storage",
	 "tabs"
  ]
}
